<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets ?>
<?import javafx.scene.control.Button ?>
<?import javafx.scene.control.ChoiceBox ?>
<?import javafx.scene.control.TableColumn ?>
<?import javafx.scene.control.TableView ?>
<?import javafx.scene.control.TextField ?>
<?import javafx.scene.control.ToolBar ?>
<?import javafx.scene.layout.AnchorPane ?>
<?import javafx.scene.layout.ColumnConstraints ?>
<?import javafx.scene.layout.GridPane ?>
<?import javafx.scene.layout.HBox ?>
<?import javafx.scene.layout.RowConstraints ?>
<?import javafx.scene.layout.VBox ?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <TableView id="fileTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <columns>
            <TableColumn id="nameColumn" prefWidth="200.0" text="Name" />
            <TableColumn id="caseColumn" prefWidth="128.0" text="Case" />
            <TableColumn id="clientColumn" prefWidth="128.0" text="Client" />
            <TableColumn id="courtColumn" prefWidth="128.0" text="Court" />
            <TableColumn id="dateColumn" prefWidth="200.0" text="Date assigned" />
          </columns>
        </TableView>
      </children>
    </AnchorPane>
    <ToolBar id="fileToolBar" prefHeight="60.0" prefWidth="200.0">
      <items>
        <Button minHeight="35.0" minWidth="75.0" mnemonicParsing="false" text="New" />
        <Button layoutX="10.0" layoutY="10.0" minHeight="40.0" minWidth="75.0" mnemonicParsing="false" text="Rename" />
        <Button layoutX="54.0" layoutY="10.0" minHeight="40.0" minWidth="75.0" mnemonicParsing="false" text="Delete" />
        <HBox minWidth="25.0" prefHeight="25.0" HBox.hgrow="ALWAYS" />
        <GridPane prefHeight="60.0" prefWidth="377.0">
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="300.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="147.0" minWidth="10.0" prefWidth="77.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <children>
            <TextField prefHeight="25.0" prefWidth="300.0" promptText="Type to filter list">
              <GridPane.margin>
                <Insets right="10.0" />
              </GridPane.margin>
            </TextField>
            <TextField prefHeight="25.0" prefWidth="300.0" promptText="Type to search for text" GridPane.rowIndex="1">
              <GridPane.margin>
                <Insets right="10.0" />
              </GridPane.margin>
            </TextField>
            <ChoiceBox prefWidth="150.0" GridPane.columnIndex="1" />
            <Button mnemonicParsing="false" prefWidth="150.0" text="Go" GridPane.columnIndex="1" GridPane.rowIndex="1" />
          </children>
        </GridPane>
      </items>
      <VBox.margin>
        <Insets />
      </VBox.margin>
    </ToolBar>
  </children>
</VBox>
